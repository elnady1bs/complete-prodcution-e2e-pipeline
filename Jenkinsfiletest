pipeline {
    agent {
        label "Jenkins-agent"
    }
    tools {
        jdk 'Java17'
        maven 'Maven3' // Ensure 'Maven3' matches your Jenkins Maven installation name
    }
    stages {
        stage("Cleanup Workspace") {
            steps {
                cleanWs() // Add parentheses to correctly invoke the cleanWs step
            }
        }

        stage("Checkout from SCM") {
            steps {
                git branch: 'main', url: 'https://github.com/elnady1bs/complete-prodcution-e2e-pipeline'
            }
        }
    }
}
